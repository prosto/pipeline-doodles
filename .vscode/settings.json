{
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "eslint.enable": true,
  "editor.formatOnSave": true,

  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "reduxdev.port": 3000,
  "jest.jestCommandLine": "pnpm test --"
}
